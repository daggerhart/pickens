{
    "name": "Pickens",
    "description": "Pickens is a simple CMS written with Slim that uses markdown files for content. Inspired by github pages.",
    "version": "0.0.1",
    "homepage": "https://github.com/daggerhart/pickens",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jonathan Daggerhart",
            "email": "jonathan@daggerhart.com",
            "homepage": "http://daggerhart.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/daggerhart/pickens/issues",
        "wiki": "https://github.com/daggerhart/pickens/wiki"
    },
    "require": {
        "erusev/parsedown": "^1.6",
        "slim/slim": "^3.0@RC",
        "symfony/yaml": "^2.7",
        "symfony/finder": "^2.7",
        "symfony/var-dumper": "^2.7",
        "symfony/options-resolver": "^2.7"
    },
    "autoload": {
        "psr-4": { "Pickens\\": "app/src/" }
    }
}
